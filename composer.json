{
    "name": "lamsa/http-requester",
    "description": "HTTP Request Handler",
    "type": "library",
    "authors": [
        {
            "name": "abdelhameed Asbahi",
            "email": "abdkwa92@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lamsa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Lamsa\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^3.10",
        "symfony/flex": "^1.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1TW989CK77ZA7B2H4HC9WAG",
            "allow-contrib": true,
            "require": "5.3.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
