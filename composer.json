{
    "name": "lamsa/http-requester",
    "description": "HTTP Request Handler",
    "type": "library",
    "authors": [
        {
            "name": "abdelhameed Asbahi",
            "email": "abdkwa92@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lamsa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Lamsa\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^3.10",
        "symfony/flex": "^1.17|^2",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "symfony/phpunit-bridge": "^6.3"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/flex": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
